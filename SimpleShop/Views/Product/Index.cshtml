@model List<Products>

<h2>Lista Produktów</h2>

<table class="table table-striped">
    <thead>

    <tr>
        <th>#</th>
        <th>Name</th>
        <th>Price</th>
    </tr>

    </thead>
    <tbody>
    
    @foreach (var product in @Model)
    {
        <tr>
            <td>@product.Id</td>
            <td>@product.Name</td>
            <td>@product.Price</td>
        </tr>
    }
    
    </tbody>

</table>
